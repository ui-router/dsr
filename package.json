{
  "name": "@uirouter/dsr",
  "description": "UI-Router Deep State Redirect: redirect to the most recently activated child state",
  "version": "1.2.0",
  "scripts": {
    "clean": "shx rm -rf lib lib-esm _bundles",
    "build": "npm run compile && rollup -c rollup.config.mjs && rollup -c rollup.config.mjs --environment MINIFY",
    "compile": "npm run clean && tsc && tsc -m es6 --outDir lib-esm",
    "test": "vitest run",
    "test:downstream": "test_downstream_projects",
    "watch": "run-p watch:*",
    "watch:buildjs": "tsc -w",
    "watch:test": "vitest",
    "changelog": "update_changelog",
    "release": "release",
    "prepublishOnly": "npm run build"
  },
  "homepage": "https://ui-router.github.io",
  "contributors": [
    {
      "name": "Chris Thielen",
      "web": "https://github.com/christopherthielen"
    }
  ],
  "maintainers": [
    {
      "name": "UIRouter Team",
      "web": "https://github.com/ui-router?tab=members"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ui-router/dsr.git"
  },
  "bugs": {
    "url": "https://github.com/ui-router/dsr/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "jsnext:main": "lib-esm/index.js",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "license": "MIT",
  "peerDependencies": {
    "@uirouter/core": ">=5.0.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.54.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/lodash": "^4.17.0",
    "@uirouter/core": "^6.1.0",
    "@uirouter/publish-scripts": "^2.6.8",
    "jsdom": "^26.1.0",
    "lodash": "^4.17.21",
    "prettier": "^3.4.0",
    "rollup": "^4.54.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "checkPeerDependencies": {
    "ignore": []
  }
}
